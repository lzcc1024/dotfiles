Xft.dpi:                96
Xft.autohint:           true
Xft.antialias:          true
Xft.rgba:               rgb
Xft.hinting:            true
Xft.hintstyle:          hintslight
Xft.lcdfilter:          lcddefault


! URxvt.iconFile: /usr/share/icons/Yaru/scalable/apps/gnome-terminal-symbolic.svg

URxvt.depth: 32
URxvt.buffered: true
URxvt.geometry:          80x24
 
URxvt.background: [95]#2E3436
URxvt.foreground: #D3D7CF
 
URxvt.color0: #002B36
URxvt.color8: #073642

URxvt.color1: #CB4B16
URxvt.color9: #DC322F

URxvt.color2: #586E75
URxvt.color10:  #859900

URxvt.color3: #657B83
URxvt.color11:  #B58900

URxvt.color4: #839496
URxvt.color12:  #268BD2

URxvt.color5: #6C71C4
URxvt.color13:  #D33682

URxvt.color6: #93A1A1
URxvt.color14:  #2AA198

URxvt.color7: #FDF6E3
URxvt.color15:  #EEE8D5

